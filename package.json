{
  "name": "marmot-ui",
  "version": "1.0.0",
  "description": "为了更好的学习工程化，还有提高编码能力，以及更多等你来发现",
  "main": "index.js",
  "scripts": {
    "dev": "webpack-dev-server --config script/webpack.dev.js",
    "build:doc": "webpack --config script/webpack.doc.js",
    "dist": "cd dist && serve",
    "lint": "eslint ./docs --ext .js,.vue,.ts,.tsx --fix"
  },
  "keywords": [
    "eslint",
    "marmot-ui",
    "webpack",
    "docs",
    "markdown"
  ],
  "author": "Gavin",
  "license": "ISC",
  "dependencies": {
    "highlight.js": "^9.16.2",
    "vue": "^2.6.10",
    "vue-router": "^3.1.3"
  },
  "sideEffects": [
    "*.less"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@babel/preset-env": "^7.7.6",
    "autoprefixer": "^9.7.3",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-preset-env": "^1.7.0",
    "css-loader": "^3.3.0",
    "eslint": "^6.7.2",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-vue": "^6.0.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.1.0",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "postcss-loader": "^3.0.0",
    "progress-bar-webpack-plugin": "^1.12.1",
    "style-loader": "^1.0.1",
    "typescript": "^3.7.3",
    "vue-loader": "^15.7.2",
    "vue-markdown-loader": "^2.4.1",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  }
}
